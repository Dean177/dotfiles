:set prompt "\ESC[33mλ > \ESC[m"
:set prompt2 "\ESC[33mλ >> \ESC[m "
:set +s -- Display the time an evaluation takes
:set +t -- print type after evaluation
:set -XOverloadedStrings

:def clear \_ -> return ":!clear"
:def test \_ -> return ":!stack test"
:def hoogle \x -> return $ ":!hoogle \"" ++ x ++ "\""
